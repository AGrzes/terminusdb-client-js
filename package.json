{
  "name": "@terminusdb/woql-client",
  "version": "1.0.0",
  "description": "TerminusDB client library",
  "main": "./dist/index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "author": "kevin@datachemist.com",
  "license": "Apache-2.0",
  "dependencies": {
    "axios": "^0.19.0",
    "eslint-plugin-react": "^7.14.3",
    "jasmine-ajax": "^4.0.0",
    "prettier": "^1.18.2",
    "save-dev": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.5.0",
    "chai": "^4.1.2",
    "cross-env": "^5.1.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "mocha": "^6.1.3",
    "nyc": "^13.3.0",
    "rimraf": "^2.6.2",
    "sinon": "^7.4.1",
    "uglify-js": "^3.6.0"
  },
  "scripts": {
    "clean": "rimraf -r dist/*",
    "test": "npm run lint && npm run cover",
    "test:prod": "cross-env BABEL_ENV=production npm run test",
    "test:only": "mocha --require babel-core/register --require babel-polyfill --recursive",
    "test:watch": "npm test -- --watch",
    "test:examples": "node examples/",
    "cover": "nyc --check-coverage --lines 55 npm run test:only",
    "lint": "eslint --fix lib/**",
    "build": "cross-env BABEL_ENV=production babel lib --out-file ./dist/woql-client.js --no-comments --source-maps ",
    "prepare": "npm run clean && npm run lint && npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/terminusdb/woql-client.git"
  },
  "keywords": [
    "Terminus",
    "WOQL",
    "Driver",
    "DB"
  ],
  "bugs": {
    "url": "https://github.com/terminusdb/woql-client/issues"
  },
  "homepage": "https://github.com/terminusdb/woql-client#readme"
}
